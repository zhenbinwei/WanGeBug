<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="titleBar">
        <attr name="title" format="string"/>
        <attr name="titleColor" format="color"/>
        <attr name="titleSize" format="dimension"/>
        <attr name="leftSrc" format="reference"/>
        <attr name="rightSrc" format="reference"/>
    </declare-styleable>
    <declare-styleable name="RatioImageView">
        <attr name="imageRatio" format="float"/>
    </declare-styleable>

    <declare-styleable name="RatioFrameLayout">
        <attr name="frameRatio" format="float"/>
    </declare-styleable>

    <declare-styleable name="LoadingView">
        <attr name="dotCount" format="integer"/>
        <attr name="dotRadius" format="integer"/>
        <attr name="dotSpace" format="integer"/>
        <attr name="dotBgColor" format="color"/>
        <attr name="dotFgColor" format="color"/>
    </declare-styleable>
</resources>